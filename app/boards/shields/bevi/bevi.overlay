#include <dt-bindings/zmk/matrix_transform.h>
/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,matrix_transform = &default_transform;
    };
    kscan0: kscan_0 {
        compatible = "zmk,kscan-gpio-matrix";
        label = "KSCAN";
        diode-direction = "col2row";
        col-gpios
            = <&pro_micro_a 3 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 2 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 1 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 0 GPIO_ACTIVE_HIGH>
            , <&pro_micro_d 15 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 10 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 8 GPIO_ACTIVE_HIGH>
            , <&pro_micro_a 9 GPIO_ACTIVE_HIGH>
            ;

        row-gpios
            = <&pro_micro_d 14 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 16 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 1 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 0 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 2 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 3 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_a 6 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 5 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_a 7 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro_d 7 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            ;
    };
     default_transform: keymap_transform_0 {
            compatible = "zmk,matrix-transform";
            columns = <10>;
            rows = <8>;
            map = <
    RC(9,4)   RC(0,0)   RC(1,0)   RC(0,1)   RC(1,1)   RC(0,2)   RC(1,2)   RC(0,3)   RC(1,3)   RC(0,4)   RC(1,4)   RC(0,5)   RC(1,5)   RC(0,6)   RC(1,6)   RC(0,7)   RC(1,7)   RC(9,7)   RC(8,5) 
    RC(8,4)   RC(2,0)   RC(3,0)   RC(2,1)    RC(3,1)    RC(2,2)   RC(3,2)   RC(2,3)   RC(3,3)   RC(2,4)   RC(3,4)   RC(2,5)   RC(3,5)   RC(2,6)   RC(3,6)   RC(2,7)   RC(3,7)    RC(7,7)  
          RC(4,0)   RC(5,0)   RC(4,1)     RC(5,1)     RC(4,2)   RC(5,2)   RC(4,3)   RC(5,3)   RC(4,4)   RC(5,4)   RC(4,5)   RC(5,5)   RC(4,6)   RC(5,6)   RC(4,7)       RC(5,7)   
    RC(9,3)   RC(6,0)   RC(7,0)   RC(6,1)       RC(7,1)     RC(6,2)   RC(7,2)   RC(6,3)   RC(7,3)   RC(6,4)   RC(7,4)   RC(6,5)   RC(7,5)   RC(6,6)   RC(7,6)        RC(6,7)      
          RC(8,0)   RC(9,0)   RC(8,1)    RC(9,1)     RC(8,2)     RC(9,2)                RC(8,3)                   RC(9,5)     RC(8,6)     RC(9,6)     RC(8,7) 
            >;
        };
};